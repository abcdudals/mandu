<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mandu.mapper.ManduMapper">

	<select id="selectName" resultType="vo.ManduVO">
		select name from mandu where id = #{id}
	</select>

	<select id="selectOne" resultType="vo.ManduVO">
		select * from mandu where id = #{id}
	</select>
	
	<insert id="insert">
		insert into mandu values(
		#{id}, #{password}, #{name}, #{myImage})
	</insert>
	
	<delete id="delete">
		delete from mandu where id = #{id}
	</delete>

</mapper>
